{
  "name": "ai-marketplace-assistant",
  "version": "1.0.0",
  "description": "AI-powered marketplace assistant bot with bilingual Spanish support",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:frontend": "webpack --mode production",
    "dev:frontend": "webpack serve --mode development",
    "watch": "tsc -w",
    "test": "echo 'Skipping tests for deployment'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "validate:core": "ts-node scripts/validate-core-functionality.ts",
    "health:check": "ts-node scripts/system-health-check.ts",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "deploy:dev": "./scripts/deploy-environment.sh dev",
    "deploy:staging": "./scripts/deploy-environment.sh staging",
    "deploy:prod": "./scripts/deploy-environment.sh prod",
    "deploy:pipeline": "./scripts/deploy-environment.sh dev --with-pipeline",
    "deploy:ec2": "npm run build && cdk deploy --context deployment-mode=ec2",
    "deploy:lambda": "npm run build && cdk deploy AIMarketplaceAssistantStack",
    "deploy:ec2-fallback": "./scripts/deploy-ec2-fallback.sh",
    "deploy:admin-dashboard": "npm run build && cdk deploy AIMarketplaceAdminDashboard-production --app 'npx ts-node bin/admin-dashboard-app.ts'",
    "dev:setup": "./scripts/dev-setup.sh",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:docker-stop": "docker-compose -f docker-compose.dev.yml down",
    "destroy": "cdk destroy",
    "destroy:ec2": "cdk destroy AIMarketplaceAssistant-EC2-Deployment",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "bootstrap": "cdk bootstrap",
    "diff": "cdk diff",
    "synth": "cdk synth"
  },
  "keywords": [
    "ai",
    "marketplace",
    "whatsapp",
    "bot",
    "spanish",
    "aws",
    "bedrock",
    "typescript"
  ],
  "author": "Luis Coy",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.450.0",
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@aws-sdk/client-cloudwatch": "^3.450.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.450.0",
    "@aws-sdk/client-dynamodb": "^3.966.0",
    "@aws-sdk/client-lambda": "^3.966.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-secrets-manager": "^3.450.0",
    "@aws-sdk/client-sns": "^3.450.0",
    "@aws-sdk/client-sqs": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.966.0",
    "@aws-sdk/s3-request-presigner": "^3.966.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "aws-cdk-lib": "^2.110.0",
    "axios": "^1.6.0",
    "constructs": "^10.3.0",
    "date-fns": "^2.30.0",
    "fuse.js": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.20.0",
    "otplib": "^12.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-select": "^5.8.0",
    "react-virtualized": "^9.22.5",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.0",
    "stripe": "^20.1.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-select": "^5.0.1",
    "@types/react-virtualized": "^9.21.29",
    "@types/socket.io-client": "^3.0.0",
    "@types/stripe": "^8.0.416",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "artillery": "^2.0.27",
    "autoprefixer": "^10.4.16",
    "css-loader": "^6.8.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fast-check": "^3.15.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
